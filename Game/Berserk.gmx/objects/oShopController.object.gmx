btS = noone;

if (global.sellout) // we've got some new sellout card right there
{
    sel = true;
    // get the result
    var parsed = json_decode(global.newSelloutStr);
    var l = parsed[? "default"];
    var t = l[| 0]; // index of the card
    var a = instance_create(sx, sy, oCardDeck);
    sI = t;
    a.type = t;
    a.mode = 3;
    a.image_xscale = 2;
    a.image_yscale = 2;
    a.font = fGame;
    with (a)
    {
        dmCardInit();
    }
    snew = l[| 1];
    snewStr = string(snew);
    sold = l[| 2];
    soldStr = string(sold);
    ds_map_destroy(parsed);
    
    btS = instance_create(sx, sy + 160, oPurchase);
    btS.type = "s";
    btS.cost = snew;
}

